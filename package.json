{
	"name": "dasby",
	"version": "1.0.0",
	"private": true,
	"description": "Dasby es un bot multiproposito de discord",
	"main": "dist/index.js",
	"scripts": {
		"pm2:delete": "pm2 delete Dasby",
		"pm2:logs": "pm2 logs Dasby --lines=200",
		"pm2:restart": "pm2 restart Dasby",
		"pm2:start": "pm2 start pm2.ecosystem.yml",
		"pm2:dev": "yarn pm2:start --env development",
		"pm2:stop": "pm2 stop Dasby",
		"update": "yarn upgrade-interactive --latest",
		"start": "node dist/index.js",
		"start:dev": "yarn build && yarn start",
		"lint": "eslint --ext ts src --fix",
		"build": "tsc -b src",
		"build:clean": "tsc -b src --clean",
		"watch": "tsc -b src -w"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dasby-project/Dasby.git"
	},
	"keywords": [],
	"author": "Phamzito",
	"contributors": [
		"gatomo-oficial"
	],
	"engines": {
		"node": ">=12.x"
	},
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/dasby-project/Husky/issues"
	},
	"homepage": "https://github.com/dasby-project/Husky#readme",
	"devDependencies": {
		"@sapphire/eslint-config": "^1.0.2",
		"@sapphire/ts-config": "^1.0.1",
		"@types/dotenv": "^8.2.0",
		"@types/ioredis": "^4.17.4",
		"@types/node": "^14.11.2",
		"@types/node-fetch": "^2.5.7",
		"@types/pg": "^7.14.5",
		"@types/ws": "^7.2.5",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.2.0",
		"eslint": "^7.8.1",
		"eslint-config-prettier": "^6.11.0",
		"eslint-formatter-pretty": "^4.0.0",
		"eslint-plugin-prettier": "^3.1.4",
		"prettier": "^2.1.2",
		"prettier-package-json": "^2.1.3",
		"typescript": "^4.0.3"
	},
	"dependencies": {
		"@influxdata/influxdb-client": "^1.6.0",
		"@influxdata/influxdb-client-apis": "^1.6.0",
		"@klasa/stopwatch": "^0.0.1",
		"@klasa/type": "^0.0.1",
		"@sapphire/decorators": "^1.0.1",
		"@sapphire/framework": "sapphire-project/framework#build",
		"@sapphire/pieces": "sapphire-project/pieces#build",
		"@sapphire/utilities": "^1.0.1",
		"@scp/in17n": "sapphirecommunityplugins/in17n#build",
		"discord.js": "discordjs/discord.js",
		"i18next": "^19.7.0",
		"ioredis": "^4.17.3",
		"module-alias": "^2.2.2",
		"pg": "^8.3.3",
		"reflect-metadata": "^0.1.13",
		"tsyringe": "^4.3.0",
		"typeorm": "0.2.28",
		"typeorm-naming-strategies": "^2.0.0",
		"zlib-sync": "^0.1.7"
	},
	"_moduleAliases": {
		"@orm": "dist/lib/orm",
		"@utils": "dist/lib/util",
		"@lib": "dist/lib",
		"@root": "dist"
	}
}
